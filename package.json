{
  "name": "expect-enzyme",
  "version": "1.1.2",
  "description": "Enzyme assertions for mjackson's Expect library",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha --opts ./mocha.opts 'src/**/*test.js'",
    "test:watch": "npm run test -- --reporter min --watch",
    "lint": "eslint src/",
    "build": "babel src/ -d dist/",
    "prepublish": "npm run build",
    "precommit": "lint-staged",
    "prettier": "prettier --single-quote --trailing-comma all"
  },
  "lint-staged": {
    "*.js": [
      "npm run prettier -- --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PsychoLlama/expect-enzyme/"
  },
  "keywords": [
    "expect",
    "assert",
    "test",
    "spec",
    "enzyme",
    "jsx",
    "react"
  ],
  "license": "MIT",
  "author": "Jesse Gibson",
  "dependencies": {
    "deep-eql": "^2.0.2",
    "react-display-name": "^0.2.0",
    "stringify-object": "^3.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-llama": "^3.0.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-react": "^7.0.1",
    "expect": "^1.20.2",
    "husky": "^0.14.3",
    "jsdom": "^11.0.0",
    "lint-staged": "^4.0.2",
    "mocha": "^3.4.2",
    "prettier": "^1.5.3",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4"
  },
  "peerDependencies": {
    "enzyme": "2.x.x",
    "expect": "1.x.x",
    "react": "13.0.0 - 15.x.x",
    "react-dom": "13.0.0 - 15.x.x"
  }
}
